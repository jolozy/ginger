<body onunload="" class="map-fullscreen page-homepage navigation-off-canvas" id="page-top">
    ​
    <!-- Outer Wrapper-->
    <div id="outer-wrapper">
        <!-- Inner Wrapper -->
        <div id="inner-wrapper">
            <!-- Navigation-->
            <div class="header">
                <div class="wrapper">
                    <div class="brand">
                        <a href="/"><img src="../../assets/img/logo.png" alt="logo"></a>
                    </div>
                    <nav class="navigation-items">
                        <div class="wrapper">
                            <ul class="main-navigation navigation-top-header"></ul>
                            <ul class="user-area">
                                <li><a href="sign-in.html">Sign In</a></li>
                                <li><a href="register.html"><strong>Register</strong></a></li>
                                <li><a href="/about">About Us</a></li>
                            </ul>
                            <div class="toggle-navigation">
                                <div class="icon" id="burger">
                                    <div class="line"></div>
                                    <div class="line"></div>
                                    <div class="line"></div>
                                </div>
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
            <!-- end Navigation-->

            <div id="page-canvas" style="padding-top: 50px; padding-bottom: 20px;">

                <div class="container">

                    <div class="row">

                        <p>
                            <ul>
                                <li><strong>Title: </strong>
                                    <%= listing.title %>
                                </li>
                                <li><strong>Address: </strong>
                                    <%= listing.location %>
                                </li>
                                <li><strong>Website: </strong>
                                    <%= listing.url %>
                                </li>
                                <li><strong>Image URL: </strong>
                                    <%= listing.gallery %>
                                </li>
                            </ul>
                        </p>

                        <h2 align="center">Edit this listing by filling up the form below:</h2>
                        <div class="col-lg-3">
                        </div>

                        <div class="col-lg-6">
                            <!-- START OF FORM FOR ADMIN'S LISTING SUBMISSION -->
                            <div class="row">
                                <form method="post" action=<%="/admin/listings/" + listing._id + "?_method=PATCH" %> >
                                    <br>
                                    <input type="string" name="category" class="form-control public-submission" placeholder="category" id="category">
                                    <br>
                                    <br>
                                    <input type="string" name="title" class="form-control public-submission" placeholder="Title" id="title">
                                    <br>
                                    <br>
                                    <input type="string" name="location" class="form-control public-submission" placeholder="Address" id="location">
                                    <br>
                                    <br>
                                    <input type="text" name="latitude" class="form-control public-submission" placeholder="latitude" id="latitude">
                                    <br>
                                    <br>
                                    <input type="text" name="longitude" class="form-control public-submission" placeholder="longitude" id="longitude">
                                    <br>
                                    <br>
                                    <input type="string" name="url" class="form-control public-submission" placeholder="Listing url" id="url">
                                    <br>
                                    <br>
                                    <input type="string" name="type" class="form-control public-submission" placeholder="type" id="type">
                                    <br>
                                    <br>
                                    <input type="string" name="type_icon" class="form-control public-submission" placeholder="type_icon" id="type_icon">

                                    <!-- <input type="boolean" name="verified" placeholder="verified" id="verified"> -->
                                    <!-- <h3>Verified?</h3>
                                    <div class="btn-group" role="group">
                                      <button id = "verified1" name ="verified" type="button" class="btn btn-danger" value=true>True</button>
                                      <button id = "verified2" name ="verified" type="button" class="btn btn-danger" value=false>False</button>
                                    </div> -->
                                    <br>
                                    <br>
                                    <input type="boolean" name="verified" class="form-control public-submission" placeholder="Verified: please enter true/false" id="verified">
                                    <br>
                                    <br>
                                    <input type="string" name="gallery" class="form-control public-submission" placeholder="image" id="type_icon">
                                    <br>
                                    <br>
                                    <div class="form-group col-md-4">
                                        <input type="hidden" name="id" value="<%= listing.id %>">
                                        <button type="submit" class="btn btn-primary col-md-12">Edit</button>
                                    </div>
                                </form>
                                <form method="post" action=<%="/admin/listings/" + listing._id + "?_method=DELETE" %> >
                                    <div class="form-group col-md-4">
                                        <button type="delete" class="btn btn-primary col-md-12" value="Delete">Delete</button>
                                    </div>
                                </form>
                                <br>
                            </div>
                            <!-- END OF ADMIN'S FORM FOR LISTING SUBMISSION -->
                        </div>
                        <div class="col-lg-3">
                        </div>

                    </div>

                </div>

            </div>
            <!-- end Container-->

            <!--Page Footer-->
            <footer id="page-footer">
              <div class="inner">
                <div class="footer-bottom">
                  <div class="container" align="center">
                    © Ginger Singapore, Inc.
                  </div>
                </div>
                <!--/.footer-bottom-->
              </div>
            </footer>
